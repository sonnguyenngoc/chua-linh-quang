<section class="block-wrapper">
  <div class="container">
    <div class="row">
      <div class="col-md-9 col-sm-8">

        <!-- block content -->
        <div class="block-content">

          <!-- grid box -->
          <div class="grid-box">
            <div class="image-slider">
              <ul class="bxslider">
                <% (1..3).each do |item| %>
                <li>
                  <div class="news-post image-post">
                    <img src="/frontend/upload/news-posts/slide1.jpg" alt="">
                    <div class="hover-box">
                      <span class="top-stories">CHÙA LINH QUANG</span>
                      <div class="inner-hover">
                        <%= link_to "Lễ Vu Lan", "", class: "category-post tech" %>
                        <h2><%= link_to "Tiêu đề slideshow", "" %></h2>
                        <ul class="post-tags">
                          <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                          <li><i class="fa fa-user"></i>bởi Chùa Linh Quang</li>
                        </ul>
                      </div>
                    </div>
                  </div>
                </li>
                <% end %>
              </ul>
            </div>
          </div>
          <!-- End grid box -->

          <!-- grid box -->
          <div class="grid-box">

            <div class="title-section">
              <h1><span>Tin Nóng</span></h1>
            </div>

            <div class="image-post-slider">
              <ul class="bxslider">
                <% (1..3).each do |item| %>
                <li>
                  <div class="news-post image-post2">
                    <div class="post-gallery">
                      <img src="/frontend/upload/news-posts/slide.jpg" alt="">
                      <div class="hover-box">
                        <div class="inner-hover">
                          <%= link_to "Tin Nóng", "", class: "category-post world" %>
                          <h2><%= link_to "Tiêu đề tin nóng", "" %></h2>
                          <ul class="post-tags">
                            <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                            <li><i class="fa fa-user"></i></i>bởi Chùa Linh Quang</li>
                          </ul>
                        </div>
                      </div>
                    </div>
                  </div>
                </li>
                <% end %>
              </ul>
            </div>

            <div class="row">
              <div class="col-md-6">
                <ul class="list-posts">
                  <% (0..11).each_with_index do |item,index| %>
                  <li>
                    <img src="/frontend/upload/news-posts/list.jpg" alt="">
                    <div class="post-content">
                      <h2><%= link_to "Đoàn Phật tử quận Bình Thạnh phát quà từ thiện", "" %></h2>
                      <ul class="post-tags">
                        <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                      </ul>
                    </div>
                  </li>
                  <%= '</ul></div><div class="col-md-6"><ul class="list-posts">'.html_safe if index%3 == 2 %>
              <% end %>
              </ul>
              </div>

            </div>

            <div class="center-button">
              <%= link_to "XEM THÊM TIN NÓNG", "" %>
            </div>

          </div>
          <!-- End grid box -->
          
          <!-- Carousel box -->
          <div class="carousel-box owl-wrapper">

            <div class="title-section">
              <h1><span>Phật Sự</span></h1>
            </div>

            <div class="owl-carousel" data-num="3">
              <% (1..5).each do |item| %>
              <div class="item news-post image-post3">
                <%= image_tag "/frontend/upload/news-posts/gal.jpg", alt: "" %>
                <div class="hover-box">
                  <h2><%= link_to "Đoàn Phật tử quận Bình Thạnh...", "", alt: "" %></h2>
                  <ul class="post-tags">
                    <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                  </ul>
                </div>
              </div>
              <% end %>
            </div>
          </div>
          <!-- End carousel box -->
          
          <!-- Carousel box -->
          <div class="carousel-box owl-wrapper">

            <div class="title-section">
              <h1><span>Phật Pháp</span></h1>
            </div>

            <div class="owl-carousel" data-num="3">
              <% (1..5).each do |item| %>
              <div class="item news-post image-post3">
                <%= image_tag "/frontend/upload/news-posts/gal.jpg", alt: "" %>
                <div class="hover-box">
                  <h2><%= link_to "Đoàn Phật tử quận Bình Thạnh...", "", alt: "" %></h2>
                  <ul class="post-tags">
                    <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                  </ul>
                </div>
              </div>
              <% end %>
            </div>
          </div>
          <!-- End carousel box -->

          <!-- Carousel box -->
          <div class="carousel-box owl-wrapper">

            <div class="title-section">
              <h1><span>Tin Tức</span></h1>
            </div>

            <div class="owl-carousel" data-num="3">
              <% (1..5).each do |item| %>
              <div class="item news-post image-post3">
                <%= image_tag "/frontend/upload/news-posts/gal.jpg", alt: "" %>
                <div class="hover-box">
                  <h2><%= link_to "Đoàn Phật tử quận Bình Thạnh...", "", alt: "" %></h2>
                  <ul class="post-tags">
                    <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                  </ul>
                </div>
              </div>
              <% end %>
            </div>
          </div>
          <!-- End carousel box -->
          
          <!-- Carousel box -->
          <div class="carousel-box owl-wrapper">

            <div class="title-section">
              <h1><span>Từ Thiện</span></h1>
            </div>

            <div class="owl-carousel" data-num="3">
              <% (1..5).each do |item| %>
              <div class="item news-post image-post3">
                <%= image_tag "/frontend/upload/news-posts/gal.jpg", alt: "" %>
                <div class="hover-box">
                  <h2><%= link_to "Đoàn Phật tử quận Bình Thạnh...", "", alt: "" %></h2>
                  <ul class="post-tags">
                    <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                  </ul>
                </div>
              </div>
              <% end %>
            </div>
          </div>
          <!-- End carousel box -->

        </div>
        <!-- End block content -->

      </div>

      <div class="col-md-3 col-sm-4">

        <!-- sidebar -->
        <div class="sidebar large-sidebar">

          <div class="widget search-widget">
            <form role="search" class="search-form">
              <input type="text" id="search" name="search" placeholder="Nhập từ khóa cần tìm.....">
              <button type="submit" id="search-submit"><i class="fa fa-search"></i></button>
            </form>
          </div>

          <div class="widget features-slide-widget">
            <div class="title-section">
              <h1><span>Văn Hóa</span></h1>
            </div>
            <div class="image-post-slider">
              <ul class="bxslider">
                <% (1..3).each do |item| %>
                <li>
                  <div class="news-post image-post2">
                    <div class="post-gallery">
                      <%= image_tag "/frontend/upload/news-posts/im.jpg", alt: "" %>
                      <div class="hover-box">
                        <div class="inner-hover">
                          <h2><%= link_to "Đoàn Phật tử quận Bình Thạnh...", "", alt: "" %></h2>
                          <ul class="post-tags">
                            <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                          </ul>
                        </div>
                      </div>
                    </div>
                  </div>
                </li>
                <% end %>
              </ul>
            </div>
            <ul class="list-posts">
              <% (1..4).each do |item| %>
              <li>
                <%= image_tag "/frontend/upload/news-posts/list.jpg", alt: "" %>
                <div class="post-content">
                  <h2><%= link_to "Đoàn Phật tử quận Bình Thạnh phát quà từ thiện", "" %></h2>
                  <ul class="post-tags">
                    <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                  </ul>
                </div>
              </li>
              <% end %>
            </ul>
          </div>
          
          <div class="widget features-slide-widget">
            <div class="title-section">
              <h1><span>GĐPT Linh Quang</span></h1>
            </div>
            <div class="image-post-slider">
              <ul class="bxslider">
                <% (1..3).each do |item| %>
                <li>
                  <div class="news-post image-post2">
                    <div class="post-gallery">
                      <%= image_tag "/frontend/upload/news-posts/im.jpg", alt: "" %>
                      <div class="hover-box">
                        <div class="inner-hover">
                          <h2><%= link_to "Đoàn Phật tử quận Bình Thạnh...", "", alt: "" %></h2>
                          <ul class="post-tags">
                            <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                          </ul>
                        </div>
                      </div>
                    </div>
                  </div>
                </li>
                <% end %>
              </ul>
            </div>
            <ul class="list-posts">
              <% (1..4).each do |item| %>
              <li>
                <%= image_tag "/frontend/upload/news-posts/list.jpg", alt: "" %>
                <div class="post-content">
                  <h2><%= link_to "Đoàn Phật tử quận Bình Thạnh phát quà từ thiện", "" %></h2>
                  <ul class="post-tags">
                    <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                  </ul>
                </div>
              </li>
              <% end %>
            </ul>
          </div>

          <div class="advertisement">
            <div class="desktop-advert">
              <span>VU LAN MÙA HIẾU HẠNH</span>
              <img src="/frontend/upload/addsense/event-250x250.jpg" alt="">
            </div>
            <div class="tablet-advert">
              <span>VU LAN MÙA HIẾU HẠNH</span>
              <img src="/frontend/upload/addsense/event-200x200.jpg" alt="">
            </div>
            <div class="mobile-advert">
              <span>VU LAN MÙA HIẾU HẠNH</span>
              <img src="/frontend/upload/addsense/event-300x250.jpg" alt="">
            </div>
          </div>

          <div class="widget tab-posts-widget">

            <ul class="nav nav-tabs" id="myTab">
              <li class="active">
                <a href="#option1" data-toggle="tab">Thư Viện Ảnh</a>
              </li>
              <li>
                <a href="#option2" data-toggle="tab">Thư Viện Nhạc</a>
              </li>
            </ul>

            <div class="tab-content">
              <div class="tab-pane active" id="option1">
                <ul class="list-posts">
                  <% (1..5).each do |item| %>
                  
                  <li>
                    <%= image_tag "/frontend/upload/news-posts/list.jpg", alt: "" %>
                    <div class="post-content">
                      <h2><%= link_to "Đoàn Phật tử quận Bình Thạnh phát.....", "" %></h2>
                      <ul class="post-tags">
                        <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                      </ul>
                    </div>
                  </li>
                  
                  <% end %>
                </ul>
              </div>
              <div class="tab-pane" id="option2">
                <ul class="list-posts">
                  <% (1..5).each do |item| %>
                  
                  <li>
                    <%= image_tag "/frontend/upload/news-posts/list.jpg", alt: "" %>
                    <div class="post-content">
                      <h2><%= link_to "Đoàn Phật tử quận Bình Thạnh phát.....", "" %></h2>
                      <ul class="post-tags">
                        <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                      </ul>
                    </div>
                  </li>
                  
                  <% end %>
                </ul>										
              </div>
            </div>
          </div>

          <div class="widget post-widget">
            <div class="title-section">
              <h1><span>Video</span></h1>
            </div>
            <% (1..2).each do |item| %>
            <div class="news-post video-post">
              <%= image_tag "/frontend/upload/news-posts/video-sidebar.jpg", alt: "" %>
              <a href="https://www.youtube.com/watch?v=OPVtUBs6YDo" class="video-link"><i class="fa fa-play-circle-o"></i></a>
              <div class="hover-box">
                <h2><%= link_to "Sự Tích Đức Phật Thích Ca 1/2", "" %></h2>
                <ul class="post-tags">
                  <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                </ul>
              </div>
              <p></p>
            </div>
            <div class="news-post video-post">
              <%= image_tag "/frontend/upload/news-posts/video-sidebar.jpg", alt: "" %>
              <a href="https://www.youtube.com/watch?v=FTQIa97H7h0" class="video-link"><i class="fa fa-play-circle-o"></i></a>
              <div class="hover-box">
                <h2><%= link_to "Sự Tích Đức Phật Thích Ca 2/2", "" %></h2>
                <ul class="post-tags">
                  <li><i class="fa fa-clock-o"></i>07/08/2016</li>
                </ul>
              </div>
              <p></p>
            </div>
            <% end %>
          </div>

        </div>
        <!-- End sidebar -->

      </div>

    </div>

  </div>
</section>